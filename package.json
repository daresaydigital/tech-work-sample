{
  "name": "Weather",
  "version": "1.0.0",
  "private": true,
  "description": "A weather app",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "start": "webpack-dev-server --debug --devtool cheap-module-source-map --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
