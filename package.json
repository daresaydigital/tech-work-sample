    "classnames": "^2.2.5",
    "moment": "^2.20.1",
    "object-assign": "^4.1.1",
    "query-string": "^5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-places-autocomplete": "^6.1.2",
    "react-pull-to-refresh": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-timeago": "^3.4.3",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-saga": "^0.16.0",
    "time-ago": "^0.2.1"
